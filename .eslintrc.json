{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": ["airbnb-base", "prettier"],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "rules": {
    "no-console": "off",
    // "indent": [
    //     "error",
    //     2
    // ],
    // "quotes": [
    //     "error",
    //     "single"
    // ],
    // "semi": [
    //     "error",
    //     "always"
    // ],
    "comma-dangle": ["error", "always-multiline"],
    "max-len": [
      "error",
      {
        "code": 90
      }
    ]
  }
}
